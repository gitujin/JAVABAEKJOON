package test1;

import java.util.Scanner;

public class Main {

	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt(); //원래의 수;
		int a1,a2,sum, temp = 0;
		temp = a; //원래의 수를 temp에 저장
		int count = 0; //사이클
		
	while(true) {
			count++;
			a1 = a/10; //두 자리 수의 왼쪽 자리
			a2 = a%10; //두 자리 수의 오른쪽 자리
			sum = a1+a2;
			
			int sum1 = a%10; // 더한 합의 오른쪽 자리
			int lastSum = a2 + sum1;
			
			System.out.println(lastSum);
			
		if(temp == lastSum) { 
			System.out.println(count);
			break;
			}
		}
	}
}