package test1;

import java.util.Scanner;

public class Main {

	public static void main(String[] args){
		int copy, a1, a2;
		int sum = 0;
		int count = 0;
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		copy = a;
		
		while(true) {
			count++;
			a1 = a/10;
			a2 = a%10;
			sum = a1+a2;
		
			a = (a2*10) + (sum%10);

			if(copy == a) {
				System.out.println(count);
				break;
			}
		
		}
		
	}
}