package test1;

import java.util.Scanner;

public class Main {

	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int max = 0;
		int min = 0;
		int[] array = new int[n];
		
		for(int i=0; i<array.length; i++) {
			array[i] = sc.nextInt();
		}
		
		for(int j=0; j<array.length; j++) {
			if(max < array[j]) {
				max = array[j];
			}
			else {
				min = array[j];
			}
		}
		
	}
}